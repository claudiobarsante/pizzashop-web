{
  "plugins": ["prettier-plugin-tailwindcss", "simple-import-sort", "import"],
  "trailingComma": "none",
  "semi": true,
  "singleQuote": true,

  "rules": {
    "simple-import-sort/imports": "error",
    "import/first": "error",
    "import/newline-after-import": "error",
    "import/no-duplicates": "error"
  }
}
